eDetail';\n//Animation\nimport { AnimatePresence } from \"framer-motion\"\n\n\n\n\nfunction App() {\n  const location = useLocation()\n  console.log(location)\n  return (\n    <div className=\"App\">\n      <GlobalStyle />\n      <Nav />\n      <AnimatePresence exitBeforeEnter>\n        <Switch location={location} key={location.pathname}>\n          <Route path=\"/\" exact>\n            <AboutUs />\n          </Route>\n          <Route path=\"/contact\">\n            <ContactUs />\n          </Route>\n          <Route path=\"/work\" exact>\n            <OurWork />\n          </Route>\n          <Route>\n            <MovieDetail path=\"/work/:id\" />\n          </Route>\n        </Switch>\n      </AnimatePresence>\n    </div>\n  );\n}\n\nexport default App;\n","/Users/hakangezer/Desktop/Neuer Ordner/capture/src/reportWebVitals.js",[],"/Users/hakangezer/Desktop/Neuer Ordner/capture/src/pages/AboutUs.js",[],"/Users/hakangezer/Desktop/Neuer Ordner/capture/src/components/AboutSection.js",[],"/Users/hakangezer/Desktop/Neuer Ordner/capture/src/components/ServicesSection.js",[],"/Users/hakangezer/Desktop/Neuer Ordner/capture/src/components/FaqSection.js",[],"/Users/hakangezer/Desktop/Neuer Ordner/capture/src/components/GlobalStyle.js",[],"/Users/hakangezer/Desktop/Neuer Ordner/capture/src/styles.js",[],"/Users/hakangezer/Desktop/Neuer Ordner/capture/src/components/Nav.js",[],"/Users/hakangezer/Desktop/Neuer Ordner/capture/src/pages/OurWork.js",[],"/Users/hakangezer/Desktop/Neuer Ordner/capture/src/pages/ContactUs.js",[],"/Users/hakangezer/Desktop/Neuer Ordner/capture/src/pages/MovieDetail.js",["84"],"import React, { useState, useEffect } from 'react';\nimport styled from 'styled-components';\nimport { useHistory } from 'react-router-dom'\nimport { MovieState } from '../movieState'\n\n//Animation\nimport { motion } from \"framer-motion\"\nimport { pageAnimation } from \"../animation\"\n\nconst MovieDetail = () => {\n    const history = useHistory();\n    const url = history.location.pathname;\n    const [movies, setMovies] = useState(MovieState);\n    const [movie, setMovie] = useState(null);\n\n    //useEffect\n    useEffect(() => {\n        const currentMovie = movies.filter((stateMovie) => stateMovie.url === url)\n        setMovie(currentMovie[0])\n\n    }, [movies, url])\n    return (\n        <>\n            {movie && (\n                <Details variants={pageAnimation} initial=\"hidden\" animate=\"show\" exit=\"exit\">\n                    <HeadLine>\n                        <h2>{movie.title}</h2>\n                        <img src={movie.mainImg} alt=\"movie\" />\n                    </HeadLine>\n                    <Awards>\n                        {movie.awards.map((award) => (\n                            <Award title={award.title} description={award.description} link={award.link} key={award.title} />\n                        ))}\n                    </Awards>\n                    <ImageDisplay>\n                        <img src={movie.secondaryImg} alt=\"second\" />\n                    </ImageDisplay>\n                </Details>\n            )}\n        </>\n    );\n};\n\nconst Details = styled(motion.div)`\ncolor: white;\n`;\n\nconst HeadLine = styled.div`\nmin-height: 90vh;\npadding-top: 20vh;\nposition: relative;\nh2{\n    position: absolute;\n    top: 10%;\n    left: 50%;\n    transform: translate(-50%, -10%)\n}\nimg{\n    width: 100%;\n    height: 70vh;\n    object-fit: cover;\n}\n@media (max-width: 500px){\n    \n    h2{\n        font-size: 3rem;\n        margin-bottom:4rem;\n    }\n\n    }\n`;\nconst Awards = styled.div`\nmin-height: 80vh;\ndisplay: flex;\nmargin: 5rem 10rem;\nalign-items: center;\njustify-content: space-around;\n@media (max-width: 1200px){\n    \n   display: block;\n   margin: 2rem 2rem;\n\n    }\n`;\n\nconst AwardStyle = styled.div`\npadding: 5rem;\nh3{\n    font-size: 2rem;\n    height:8rem\n}\n.line{\n    width: 100%;\n    background: #23d997;\n    height: .5rem;\n    margin: 1rem 0rem;\n}\np{\n    padding: 2rem 0rem;\n}\n`;\n\nconst ImageDisplay = styled.div`\nmin-height: 50vh;\nimg{\n    width: 100%;\n    height: 100vh;\n    object-fit: cover;\n}\n`\n\n//Awards Component\nconst Award = ({ title, description, link }) => {\n    return (\n        <AwardStyle>\n            <h3>\n                {title}\n            </h3>\n            <div className=\"line\"></div>\n            <p>{description}</p>\n            <StyledLink href={link} target=\"__blank\">Go to Site</StyledLink>\n\n        </AwardStyle>\n    );\n};\nconst StyledLink = styled.a`\ntext-decoration: none;\nfont-weight: bold;\n    font-size: 1.1.rem;\n    cursor: pointer;\n    padding: 1rem 2rem;\n    border: 3px solid #23d997;\n    background: transparent;\n    color: white;\n    transition: all .5s ease;\n    font-family: 'Inter', sans-serif;\n\n    &:hover{\n        background-color: #23d997;\n        color: white;\n    }\n`;\n\n\n\nexport default MovieDetail;","/Users/hakangezer/Desktop/Neuer Ordner/capture/src/movieState.js",[],"/Users/hakangezer/Desktop/Neuer Ordner/capture/src/animation.js",[],"/Users/hakangezer/Desktop/Neuer Ordner/capture/src/components/Wave.js",[],"/Users/hakangezer/Desktop/Neuer Ordner/capture/src/components/Toggle.js",[],"/Users/hakangezer/Desktop/Neuer Ordner/capture/src/components/useScroll.js",[],"/Users/hakangezer/Desktop/Neuer Ordner/capture/src/components/ScrollTop.js",[],{"ruleId":"85","replacedBy":"86"},{"ruleId":"87","replacedBy":"88"},{"ruleId":"89","severity":1,"message":"90","line":9,"column":8,"nodeType":"91","messageId":"92","endLine":9,"endColumn":17},{"ruleId":"89","severity":1,"message":"93","line":13,"column":20,"nodeType":"91","messageId":"92","endLine":13,"endColumn":29},"no-native-reassign",["94"],"no-negated-in-lhs",["95"],"no-unused-vars","'ScrollTop' is defined but never used.","Identifier","unusedVar","'setMovies' is assigned a value but never used.","no-global-assign","no-unsafe-negation"]